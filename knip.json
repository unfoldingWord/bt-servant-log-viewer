{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "workspaces": {
    ".": {
      "entry": ["tooling/**/*.js"],
      "project": ["tooling/**/*.js"]
    },
    "apps/web": {
      "entry": ["src/routes/**/*.{svelte,ts}", "src/lib/**/*.{svelte,ts}", "src/app.html"],
      "project": ["src/**/*.{svelte,ts}"],
      "paths": {
        "$lib": ["./src/lib"],
        "$lib/*": ["./src/lib/*"]
      }
    },
    "apps/api": {
      "entry": [],
      "project": ["src/**/*.ts"]
    },
    "packages/*": {
      "entry": [],
      "project": ["src/**/*.ts"]
    }
  },
  "ignore": ["**/*.test.ts", "**/*.spec.ts", "**/dist/**", "**/build/**", "**/.svelte-kit/**"],
  "ignoreDependencies": ["tslib", "@types/chai"],
  "ignoreBinaries": ["fly"]
}
